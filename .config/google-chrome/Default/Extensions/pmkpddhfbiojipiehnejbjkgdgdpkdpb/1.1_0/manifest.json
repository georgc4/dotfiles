{
   "background": {
      "persistent": false,
      "scripts": [ "src/background.js" ]
   },
   "commands": {
      "hide": {
         "description": "hide/show controls",
         "suggested_key": {
            "default": "Ctrl+M"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "src/hide.js", "src/show.js" ],
      "matches": [ "http://*.youtube.com/watch?*", "https://*.youtube.com/watch?*" ]
   } ],
   "description": "Press Ctrl+M to hide or show the YouTube video player controls.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "hyde â€” hide the YouTube video player controls",
   "permissions": [ "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
